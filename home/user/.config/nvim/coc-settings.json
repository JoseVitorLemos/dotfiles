{
  "languageserver": {
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    }
  },
  "typescript.showUnused": false,
  "javascript.showUnused": false,
  "diagnostic.enable": false ,
  "coc.preferences.diagnostic.enableMessa": false,
  "diagnostic.enableMessage": false,
}
